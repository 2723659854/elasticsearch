{
    "name": "xiaosongshu/elasticsearch",
    "description": "an elasticsearch sdk for php.",
    "type": "library",
    "license": "mit",
    "autoload": {
        "psr-4": {
            "Xiaosongshu\\Elasticsearch\\": "src/"
        }
    },
    "authors": [
        {
            "name": "yanglong",
            "email": "2723659854@qq.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.3.0",
        "elasticsearch/elasticsearch": "^7.17",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "9.6.x-dev"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
  "scripts": {
    "post-install-cmd": [
      "\\Xiaosongshu\\Elasticsearch\\Install::install"
    ],
    "post-update-cmd": [
      "\\Xiaosongshu\\Elasticsearch\\Install::install"
    ],
    "pre-remove-cmd": [
      "\\Xiaosongshu\\Elasticsearch\\Install::uninstall"
    ]
  }
}
